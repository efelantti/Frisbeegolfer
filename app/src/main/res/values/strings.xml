<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Name of the application. Used in the manifest file. -->
    <string name="app_name">Frisbeegolfer</string>

    <!-- Name of the button in the start screen that allows the user to start a new round. -->
    <string name="button_newRound_text">New Round</string>

    <!-- Name of the button in the start screen that allows the user to list, add and edit courses. -->
    <string name="button_courses_text">Courses</string>

    <!-- Name of the button in the start screen that allows the user to list, add and edit players. -->
    <string name="button_players_text">Players</string>

    <!-- Name of the button in the start screen that navigates to the statistics screen. -->
    <string name="button_statistics_text">Statistics</string>

    <!-- Name of the button in the start screen that takes the user to the settings menu. -->
    <string name="button_settings_text">Settings</string>

    <!-- Name of the button in the players screen that allows the user to add a new player. -->
    <string name="add_player">Add player</string>

    <!-- Text for save button. -->
    <string name="button_save">Save</string>

    <!-- String formatting for full name when nickname is available. Used in when listing players. -->
    <string name="fullname_with_nickname"><xliff:g id="first name" example="John">%1$s</xliff:g> \"<xliff:g id="nick name" example="Johnny">%2$s</xliff:g>\" <xliff:g id="last name" example="Smith">%3$s</xliff:g></string>

    <!-- String formatting for full name when nickname is not available. Used in when listing players. -->
    <string name="fullname_without_nickname"><xliff:g id="first name" example="John">%s</xliff:g> <xliff:g id="last name" example="Smith">%s</xliff:g></string>

    <!-- Formatting for how email is displayed. Includes the label. -->
    <string name="email_descriptor">Email: <xliff:g id="email" example="example@example.com">%s</xliff:g></string>

    <!-- Label for first name field. -->
    <string name="hint_word_first_name">First name</string>

    <!-- Label for nickname field. -->
    <string name="hint_word_nickname">Nickname</string>

    <!-- Label for email field. -->
    <string name="hint_word_email">Email</string>

    <!-- Label for last name field. -->
    <string name="hint_word_lastname">Last name</string>

    <!-- Error message when a field is not filled correctly. Separated in the message and name of the field which is invalid. -->
    <string name="invalid_field">Please enter a valid <xliff:g id="Field name" example="First name">%1$s</xliff:g></string>

    <!-- Title for the screen that allows to add a new player. -->
    <string name="text_activity_new_player_title_add">Add player</string>

    <!-- Title for the screen that allows to edit a player. -->
    <string name="text_activity_new_player_title_edit">Edit player</string>

    <!-- Label for cancel button. -->
    <string name="button_cancel">Cancel</string>

    <!-- Label for a positive confirmation button. -->
    <string name="button_yes">Yes</string>

    <!-- Label for a negative confirmation button. -->
    <string name="button_no">No</string>

    <!-- Label for edit button in the action bar. -->
    <string name="action_edit">Edit</string>

    <!-- Title for the players screen. -->
    <string name="players_activity_title">Players</string>

    <!-- Error message that is displayed when one or more fields are invalid. -->
    <string name="error_message_requiredFields">Please verify validity of fields.</string>

    <!-- Message that is shown instead of a player list, when the database contains no players. -->
    <string name="no_players_available">There are no players added yet.</string>

    <!-- Message that is shown instead of a player list, when the database contains no players. -->
    <string name="no_players_available_when_choosing">No players found. Please add at least one player first.</string>

    <!-- Error message that displayed when user tries to create a new player with information that already exists in database. -->
    <string name="error_duplicate_player">A player with same details already exists!</string>

    <!-- Message that is displayed when user tries to save, but no data is modified. -->
    <string name="player_data_not_edited">No value was changed</string>

    <!-- Message that is displayed on the appbar when a player is selected from the player listing. -->
    <string name="player_selected">Player selected</string>

    <!-- Name of the close action in the appbar. -->
    <string name="action_close">Close</string>

    <!-- Name of the save action in the appbar. -->
    <string name="action_save">Save</string>

    <!-- Message that is shown instead of a course list, when the database contains no courses. -->
    <string name="no_courses_available">There are no courses added yet.</string>

    <!-- Message that is shown instead of a course list, when the database contains no courses, and user is trying to start a new round. -->
    <string name="no_courses_available_when_choosing">No courses found. Please create at least one course.</string>

    <!-- Name of the FAB button for adding a course. -->
    <string name="add_course">Add a course</string>

    <!-- Title for the courses screen. -->
    <string name="courses_activity_title">Courses</string>

    <!-- Error message that displayed when user tries to create a new course with information that already exists in database. -->
    <string name="error_duplicate_course">A course with same details already exists!</string>

    <!-- Message that is displayed when a course is selected. -->
    <string name="course_selected">Course selected</string>

    <!-- String for course name (label + name). -->
    <string name="courseName"><xliff:g id="courseName" example="Laajavuori Frisbeegolf park">%s</xliff:g></string>

    <!-- String for city name (label + city). -->
    <string name="city"><xliff:g id="city" example="Jyväskylä">%s</xliff:g></string>

    <!-- String for number of holes (label + amount). -->
    <string name="numberOfHoles">Holes: <xliff:g id="numberOfHoles" example="21">%d</xliff:g></string>

    <!-- Title for screen that allows to add a new course. -->
    <string name="text_activity_new_course_title_add">New course</string>

    <!-- Title for screen that allows to edit a course. -->
    <string name="text_activity_new_course_title_edit">Edit course</string>

    <!-- Label for field "Course name". -->
    <string name="hint_word_course_name">Course name</string>

    <!-- Label for field "City". -->
    <string name="hint_word_city">City</string>

    <!-- Title for dialog that asks user confirmation to overwrite data. -->
    <string name="dialog_title_overwrite">Overwrite</string>

    <!-- The message that is displayed to the user before overwriting data. -->
    <string name="dialog_message_confirm_overwrite">Are you sure you want to overwrite existing data? Please note that previous data can not be recovered.</string>

    <!-- Title for dialog that asks user confirmation before canceling. -->
    <string name="dialog_title_cancel">Cancel</string>

    <!-- The message that is displayed to the user before canceling from the current screen. -->
    <string name="dialog_message_cancel">Are you sure you want to cancel? Any unsaved data will be lost.</string>

    <!-- Subtitle for course data section in the course screen. -->
    <string name="new_course_fragment_header_course_data">Course</string>

    <!-- Subtitle for holes data section in the course screen. -->
    <string name="new_course_fragment_header_holes_data">Holes</string>

    <!-- Label for "Number of holes" field, which the user can use to change amount of holes. -->
    <string name="hint_word_number_of_holes">Number of holes</string>

    <!-- Name of button that allows the user to change the number of holes (after specifying amount in the proper field). -->
    <string name="create_holes">Apply</string>

    <!-- Label for field "Length (meters)" ie., the length of the hole. -->
    <string name="hint_word_length">Length (meters)</string>

    <!-- Label for field "Par". -->
    <string name="hint_word_par">Par</string>

    <!-- Header for field "Hole number". -->
    <string name="hole_number_header">#</string>

    <!-- Header for field "Par". -->
    <string name="par_count_header">Par</string>

    <!-- Error message that is displayed when user tries to set invalid amount of holes. -->
    <string name="invalid_number_of_holes" formatted="true">Only values in the range 1-<xliff:g id="maximumNumberOfHoles" example="99">%d</xliff:g> are valid.</string>

    <!-- Title for dialog that is displayed to the user before removing holes. -->
    <string name="remove_holes">Remove holes?</string>

    <!-- Message that is displayed to the user when asking confirmation about removing holes (when setting a smaller amount of holes that currently shown). -->
    <string name="dialog_message_confirm_remove_holes">Proceeding will delete holes from the end of the list. Are you want to continue?</string>

    <!-- Content description for FAB that allows user to select a course for a round. -->
    <string name="select_course">Select course</string>

    <!-- Content description for FAB that allows user to select the players for a round. -->
    <string name="select_players">Select players</string>

    <!-- Title for fragment that allows the player to choose a course for a round. -->
    <string name="choose_a_course_title">Choose a course</string>

    <!-- Title for fragment that allows the player to choose players for a round. -->
    <string name="choose_players_title">Choose players</string>

    <!-- String that is displayed on the app bar when user has selecte at least one player for a round. -->
    <plurals name="numberPlayersSelected">
        <item quantity="one">%d player selected</item>
        <item quantity="other">%d players selected</item>
    </plurals>
</resources>